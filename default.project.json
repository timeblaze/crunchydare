{
  "name": "crunchydare",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "Generated": {
        "$className": "Folder",
        "$path": "generated"
      },
      "Packages": {
        "$className": "Folder",
        "$path": "roblox_packages"
      },
      "SharedPackages": {
        "$className": "Folder",
        "$path": "shared_packages"
      },
      "$path": "src/ReplicatedStorage"
    },
    "ServerScriptService": {
      "$path": "src/ServerScriptService"
    },
    "ServerStorage": {
      "$path": "src/ServerStorage"
    },
    "StarterPlayer": {
      "$path": "src/StarterPlayer"
    }
  },
  "globIgnorePaths": [
    "**/*.txt",
    "**/*.toml"
  ],
  "syncbackRules": {
    "ignorePaths": [
      "src/**/*.luau",
      "roblox_packages/**",
      "wally.toml",
      "wally.lock",
      "pesde.toml",
      "pesde.lock",
      "injected/**",
      "generated/**"
    ]
  }
}